VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "Form_ConfiguracaoBancoDados"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub cmdclose_Click()
    DoCmd.Close acForm, Me.Name
End Sub

Private Sub cmdSalvar_Click()
On Error GoTo catch
    DoCmd.RunCommand acCmdSaveRecord
    Dim cn As New Connect
    Call cn.OpenConnection
    MsgBox "Salvo e testado com sucesso!", , "Teste"
    Exit Sub
catch:
    MsgBox Err.Description, vbCritical, "Atenção"
End Sub

Private Sub Comando46_Click()
    MsgBox "Caso você possua um formulário modelo ele crirá nesse formuário os campos das classes para você.", vbInformation, "O que é?"
End Sub
