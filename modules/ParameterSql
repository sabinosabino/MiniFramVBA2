VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "ParameterSql"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Public CollectionFields As Collection
Public CollectionValues As Collection
Public NameTable As String

Private Sub Class_Initialize()
    Set CollectionFields = New Collection
    Set CollectionValues = New Collection
End Sub
Private Sub Class_Terminate()
    Set CollectionFields = Nothing
    Set CollectionValues = Nothing
End Sub

